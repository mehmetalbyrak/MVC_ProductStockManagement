@using MVC_ProductStockManagement.Models.Entity
@model List<Category>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class = "table table-bordered">
    <tr>
        <th>Category Id</th>
        <th>Category Name</th>
    </tr>

    <tbody>
        @foreach(var category in Model)
            {
                <tr>
                    <th>
                        @category.CategoryId
                    </th>
                    <td>
                        @category.CategoryName
                    </td>
                </tr>
            }
    </tbody>
</table>

